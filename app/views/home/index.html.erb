<% if user_signed_in? %>
	<h1>You're Logged in</h1>
  <%= link_to "Logout", destroy_user_session_path, data: { turbo_method: :delete }, class: 'btn btn-primary' %>
<% else %>
	<h1>Hello World</h1>
	<%= link_to "Sign Up", new_user_registration_path, class: 'btn btn-primary btn-lg' %>
	<%= link_to "Login in", new_user_session_path, class: 'btn btn-outline-primary btn-lg' %>
<%end%>